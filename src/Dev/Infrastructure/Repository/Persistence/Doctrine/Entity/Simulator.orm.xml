<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping
        xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="DevBodas\Dev\Domain\Entity\Simulator" table="simulator">
        <id name="id" type="IdType" >
            <generator strategy="NONE" />
        </id>
        <field name="name" type="text" />
        <field name="number" type="integer" />
        <field name="direction" type="text"/>
        <field name="route" type="integer" />
        <field name="date" type="text" />
    </entity>

</doctrine-mapping>
